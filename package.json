{
  "name": "@digital-boss/n8n-nodes-binance",
  "version": "0.1.0",
  "description": "Binance support for n8n.",
  "license": "Apache 2.0",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Saladin Dacic",
    "email": "saladindacic@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaladinDacic/Binance.git"
  },
  "keywords": [
    "n8n",
    "nodemation",
    "dnc",
    "digital north consulting",
    "digital boss",
    "nodes",
    "custom",
    "workflow",
    "customer service"
  ],
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "genversion -s -e nodes/version.ts && git add . && tsc && gulp",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "nodelinter": "nodelinter",
    "watch": "tsc --watch",
    "test": "jest",
    "release": "npm run build && npm publish --access=public",
    "version": "npm run build",
    "postversion": "git push"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/BinanceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Binance/Binance.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.1",
    "@types/request-promise-native": "~1.0.18",
    "genversion": "^3.1.1",
    "gulp": "^4.0.2",
    "jest": "^28.1.3",
    "n8n-workflow": "^0.109.0",
    "nodelinter": "^0.1.19",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4"
  },
  "dependencies": {
    "n8n-core": "^0.127.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "flippin-sweet"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
